    ed->EDF [ label = "f" ];
    ed->IDENT [ label = "\'A\'..\'Z\' | \'a\'..\'e\' | \'g\'..\'z\' | \'0\'..\'9\' | _ " ];
    EDF->IDENT [ label = "\'A\'..\'Z\' | \'a\'..\'z\' | \'0\'..\'9\' | _ " ];
    
